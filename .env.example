# Configuración de Google Gemini
GOOGLE_API_KEY=tu-api-key-aqui

# Token del Bot de Telegram
# Obtén tu token desde @BotFather en Telegram
TELEGRAM_BOT_TOKEN=tu-token-de-telegram-aqui

# Configuración de Odoo MCP
# Habilitar integración con Odoo (true/false)
ODOO_MCP_ENABLED=false

# Ruta al script del servidor MCP de Odoo
# Ejemplo: /path/to/odoo_mcp_server.py
ODOO_MCP_SERVER_PATH=
